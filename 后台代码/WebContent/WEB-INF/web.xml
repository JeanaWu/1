<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>IQTest</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>SelectIQQuestion</servlet-name>
    <servlet-class>cn.IQTest.Application.Controllor.SelectIQQuestionControllor</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SelectIQQuestion</servlet-name>
    <url-pattern>/SelectIQQuestion</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>InsertIQQuestion</servlet-name>
    <servlet-class>cn.IQTest.Application.Controllor.InsertIQQuestionControllor</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InsertIQQuestion</servlet-name>
    <url-pattern>/InsertIQQuestion</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DeleteIQQuestion</servlet-name>
    <servlet-class>cn.IQTest.Application.Controllor.DeleteIQQuestionControllor</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteIQQuestion</servlet-name>
    <url-pattern>/DeleteIQQuestion</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Admin</servlet-name>
    <servlet-class>cn.IQTest.Application.Controllor.AdminController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Admin</servlet-name>
    <url-pattern>/Admin</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>cn.IQTest.Application.Controllor.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>selectQuestionList</servlet-name>
    <servlet-class>cn.IQTest.Application.Controllor.SelectIQQuestionListControllor</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>selectQuestionList</servlet-name>
    <url-pattern>/selectQuestionList</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>scoreController</servlet-name>
    <servlet-class>cn.IQTest.Application.Controllor.ScoreController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>scoreController</servlet-name>
    <url-pattern>/scoreController</url-pattern>
  </servlet-mapping>
  
</web-app>